{
  "name": "@lloydk/tsort",
  "version": "0.5.0",
  "description": "A Typescript port of the Ruby TSort module",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/tsort.umd.cjs",
  "module": "./dist/tsort.js",
  "exports": {
    ".": {
      "import": "./dist/tsort.js",
      "require": "./dist/tsort.umd.cjs"
    }
  },
  "types": "./dist/tsort.d.ts",
  "scripts": {
    "doc": "typedoc src/index.ts",
    "build": "tsc && vite build",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lloydk/tsort.git"
  },
  "keywords": [
    "tsort",
    "tarjan",
    "scc",
    "strongly connected components"
  ],
  "author": "Lloyd Kupchanko",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/lloydk/tsort/issues"
  },
  "homepage": "https://github.com/lloydk/tsort#readme",
  "devDependencies": {
    "@vitest/coverage-c8": "^0.31.4",
    "@vitest/ui": "^0.31.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.4"
  },
  "packageManager": "pnpm@8.6.12"
}
